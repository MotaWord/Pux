        "universal/universal": "2.0.x-dev"
    },
    "license": "MIT",
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Pux\\": "src/"
        }
    },
    "extra": {
        "branch-alias": { "dev-master": "2.0.x-dev" }
    }
}
